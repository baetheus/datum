{
  "name": "@nll/datum",
  "version": "3.0.1",
  "description": "ADT for handling fetchable and refreshable data",
  "files": [
    "*"
  ],
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build-tsc": "tsc",
    "build-prepare": "ts-node --skip-project ./scripts/prepare.ts",
    "build-notify": "echo \"To publish run: npm publish --prefix 'dist'\"",
    "build": "npm run test && npm run docs && npm run build-tsc && npm run build-prepare && npm run build-notify",
    "docs": "npx docs-ts",
    "jest-clear-cache": "jest --clearCache",
    "jest": "jest --ci",
    "coveralls": "cat ./coverage/lcov.info | npx coveralls",
    "release": "npx standard-version && npm run build",
    "test": "npm run jest-clear-cache && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nullpub/datum.git"
  },
  "keywords": [
    "fp-ts",
    "adt",
    "AsyncData"
  ],
  "author": "Brandon Blaylock <brandon@null.pub> (https://blaylock.dev)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/nullpub/datum/issues"
  },
  "homepage": "https://github.com/nullpub/datum#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.7",
    "babel-jest": "^25.1.0",
    "coveralls": "^3.0.9",
    "docs-ts": "^0.3.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "fp-ts": "^2"
  },
  "prettier": {
    "singleQuote": true
  }
}
